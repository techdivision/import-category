{
  "magento-edition": "CE",
  "magento-version": "2.3.0",
  "operation-name" : "add-update",
  "archive-artefacts" : true,
  "debug-mode" : false,
  "entity-type-code" : "catalog_category",
  "listeners" : [
    {
      "app.set.up" : [
        "import.listener.render.ansi.art",
        "import.listener.initialize.registry"
      ]
    },
    {
      "app.tear.down" : [
        "import.listener.clear.registry"
      ]
    }
  ],
  "databases" : [
  ],
  "loggers": [
    {
      "name": "system",
      "channel-name": "logger/system",
      "type": "Monolog\\Logger",
      "handlers": [
        {
          "type": "Monolog\\Handler\\ErrorLogHandler",
          "formatter": {
            "type": "Monolog\\Formatter\\LineFormatter",
            "params" : [
              {
                "format": "[%datetime%] %channel%.%level_name%: %message% %context% %extra%",
                "date-format": "Y-m-d H:i:s",
                "allow-inline-line-breaks": true,
                "ignore-empty-context-and-extra": true
              }
            ]
          }
        }
      ],
      "processors": [
        {
          "type": "Monolog\\Processor\\MemoryPeakUsageProcessor"
        }
      ]
    }
  ],
  "operations" : [
    {
      "name" : "delete",
      "plugins" : [
        {
          "id": "import.plugin.global.data"
        },
        {
          "id": "import.plugin.subject",
          "subjects" : [
            {
              "id": "import.subject.move.files",
              "file-resolver": {
                "prefix": "category-import"
              },
              "ok-file-needed": true
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-import"
              },
              "observers": [
                {
                  "import": [
                    "import_category.observer.clear.category"
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "import.plugin.archive"
        }
      ]
    },
    {
      "name" : "replace",
      "plugins" : [
        {
          "id": "import.plugin.global.data"
        },
        {
          "id": "import.plugin.subject",
          "subjects": [
            {
              "id": "import.subject.move.files",
              "file-resolver": {
                "prefix": "category-import"
              },
              "ok-file-needed": true
            },
            {
              "id": "import_category.subject.sortable.bunch",
              "create-imported-file": false,
              "file-resolver": {
                "prefix": "category-import"
              },
              "listeners": [
                {
                  "subject.export.start": [
                    "import_category.listener.sort.category"
                  ]
                }
              ],
              "observers": [
                {
                  "import": [
                    "import_category.observer.category.copy"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-import"
              },
              "observers": [
                {
                  "import": [
                    "import_category.observer.clear.category"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-create"
              },
              "params" : [
                {
                  "copy-images" : false
                }
              ],
              "observers": [
                {
                  "import": [
                    "import_category.observer.composite.create.replace"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-path"
              },
              "observers": [
                {
                  "import": [
                    "import_category.observer.category.path"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-url-rewrite"
              },
              "observers": [
                {
                  "import": [
                    "import_category.observer.url.rewrite"
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "import_category.plugin.children.count"
        },
        {
          "id": "import.plugin.archive"
        }
      ]
    },
    {
      "name" : "add-update",
      "plugins" : [
        {
          "id": "import.plugin.global.data"
        },
        {
          "id": "import.plugin.subject",
          "subjects": [
            {
              "id": "import.subject.move.files",
              "file-resolver": {
                "prefix": "category-import"
              },
              "ok-file-needed": true
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-import"
              },
              "listeners": [
                {
                  "plugin.export.success": [
                    "import_category.listener.sort.category"
                  ]
                }
              ]
              "observers": [
                {
                  "import": [
                    "import_category.observer.category.copy"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-create"
              },
              "params" : [
                {
                  "copy-images" : false
                }
              ],
              "observers": [
                {
                  "import": [
                    "import_category.observer.composite.add_update"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-path"
              },
              "observers": [
                {
                  "import": [
                    "import_category.observer.category.path"
                  ]
                }
              ]
            },
            {
              "id": "import_category.subject.bunch",
              "file-resolver": {
                "prefix": "category-url-rewrite"
              },
              "observers": [
                {
                  "import": [
                    "import_category.observer.url.rewrite.update"
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "import_category.plugin.children.count"
        },
        {
          "id": "import.plugin.archive"
        }
      ]
    }
  ]
}