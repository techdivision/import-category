{
  "header-mappings": {
    "catalog_category": {
      "image_path": "image"
    }
  },
  "shortcuts": {
    "ce": {
      "catalog_category": {
        "delete": [
          "general/general/move-files",
          "general/general/global-data",
          "ce/catalog_category/delete"
        ],
        "replace": [
          "general/general/move-files",
          "general/general/cache-warmer",
          "general/general/global-data",
          "ce/catalog_category/sort",
          "ce/catalog_category/delete",
          "ce/catalog_category/replace",
          "ce/catalog_category/replace.path",
          "ce/catalog_category/replace.url-rewrite",
          "general/catalog_category/children-count"
        ],
        "add-update": [
          "general/general/move-files",
          "general/general/cache-warmer",
          "general/general/global-data",
          "ce/catalog_category/sort",
          "ce/catalog_category/add-update",
          "ce/catalog_category/add-update.path",
          "ce/catalog_category/add-update.url-rewrite",
          "general/catalog_category/children-count"
        ]
      }
    }
  },
  "operations": {
    "general": {
      "catalog_category": [
        {
          "name": "children-count",
          "plugins": [
            {
              "id": "import_category.plugin.children.count"
            }
          ]
        },
        {
          "name": "validate",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "listeners": [
                {
                  "plugin.process.success": [
                    "import.listener.stop.validation"
                  ]
                }
              ],
              "subjects": [
                {
                  "id": "import.subject.validator",
                  "create-imported-file": false,
                  "file-resolver": {
                    "prefix": "category-import"
                  },
                  "listeners": [
                    {
                      "subject.artefact.header.row.process.start": [
                        "import.listener.validate.header.row"
                      ]
                    }
                  ],
                  "params" : {
                    "custom-validations" : {
                      "path" :  [ "/.+/" ]
                    }
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.composite.base.validate"
                      ]
                    }
                  ],
                  "callbacks": [
                    {
                      "path": [ "import.callback.custom.regex.validator" ],
                      "store_view_code": [ "import.callback.store.view.code.validator" ],
                      "attribute_set_code": [ "import.callback.attribute.set.name.validator" ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "convert",
          "plugins": [
            {
              "id": "import_converter.plugin.exportable.converter.generic",
              "params": {
                "exportable-artefact-types": [
                  "category-import"
                ]
              },
              "listeners": [
                {
                  "plugin.export.start": [
                    "import_converter_product_category.listeners.reduce.category"
                  ]
                }
              ],
              "subjects": [
                {
                  "id": "import_converter_product_category.subject.converter",
                  "create-imported-file": false,
                  "file-resolver": {
                    "prefix": "product-import"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_converter_product_category.observer.product.to.category"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    "ce": {
      "catalog_category": [
        {
          "name": "sort",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
	            {
	              "id": "import_category.subject.sortable.bunch",
	              "create-imported-file": false,
	              "file-resolver": {
	                "prefix": "category-import"
	              },
	              "listeners": [
	                {
	                  "subject.export.start": [
	                    "import_category.listener.sort.category"
	                  ]
	                }
	              ],
	              "observers": [
	                {
	                  "import": [
	                    "import_category.observer.category.copy"
	                  ]
	                }
	              ]
	            }
	          ]
	        }
	      ]
        },
        {
          "name": "delete",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-import"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.clear.category"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-create"
                  },
                  "params": {
                    "copy-images": false
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.composite.create.replace"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace.path",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-path"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.category.path"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace.url-rewrite",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-url-rewrite"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.url.rewrite"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-create"
                  },
                  "params": {
                    "copy-images": false
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.composite.add_update"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update.path",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-path"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.category.path"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update.url-rewrite",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-url-rewrite"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category.observer.url.rewrite.update"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  }
}